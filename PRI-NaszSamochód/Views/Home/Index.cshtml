    <body ng-app="myApp">
        <div class="main-flex">
      
            <ul class="nav">
                <li class="search">
                        <input type="search"/>
                </li>
                <li><a ui-sref="User/UserPage" ui-sref-active="active">  @Model.User.Name @Model.User.Surname  </a></li>
                <li><a href="#"><div class="fao" id="user"></div></a></li>
                <li><a href="#"><div class="fao" id="envelope"></div></a></li>
                <li><a href="#"><div class="fao" id="bell"></div></a></li>
                <li><div class="dropdown">
                        <div class="fao" id="signout" ng-controller="LogOffController">
                            <a ng-click ="LogOff()" >Logout</a>
                        </div>
                       
                    </div>
                </li>
            </ul>
            
                <header>
                    <div ui-view name="header"></div>
                    <p id="headerPar"></p>
                </header>

                <div class="child-flex">

                    <aside id="nav">
                        <ul>
                            <li><a href="#">Znajomi</a></li>
                            <li><a href="#">Wiadomości</a></li>
                            <li><a href="#Groups/AddNewGroup" id="addNewGroupClicker">Utwórz nową grupę</a></li>
                            <li>
