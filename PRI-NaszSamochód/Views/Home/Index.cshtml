                                    <option value="default" selected>Twoje grupy: </option>
                                    <option value="group1">Volvo fajne jest</option>
                                    <option value="group2">Fiat</option>
                                </select>
                            </li>
                        </ul>
                    </aside>

                    <aside id="chat">
